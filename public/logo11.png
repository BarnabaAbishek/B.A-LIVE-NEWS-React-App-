                        circle.style.strokeDashoffset = `${progress}`;

                        var progressCircle = document.querySelector( '#progress-circle' );
                        if ( window.scrollY <= window.innerHeight ) {
                            progressCircle.style.display = "none";
                        } else {
                            progressCircle.style.display = "block";
                        }
