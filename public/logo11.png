document.addEventListener("DOMContentLoaded", function() {

            var quoteboxes = document.querySelectorAll(".quotebox");
            if (quoteboxes.length > 0) {
                quoteboxes.forEach(function(quotebox) {
                    var imageHTML = '<img src="https://www.wordstream.com/wp-content/uploads/2023/07/start-quote-orange.png" alt="quote icon" width="300">';
                    // Insert the image HTML as the first child of the quotebox div
                    quotebox.insertAdjacentHTML('afterbegin', imageHTML);
