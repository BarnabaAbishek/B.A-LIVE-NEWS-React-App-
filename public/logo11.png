            }

            // grab sidebar and assign to variable
            var sidebar = document.getElementById("tableofcontent");

            var hasSidebar = false;

            if ( sidebar != null ) {
                hasSidebar = true;


                // grab all "chapters" in table of contents (specifically, the <a> tags)
                var chapterLinks = document.querySelectorAll("[data-chapter]");

                // build array of content links at the beginning of each "chapter", referencing the Table of Contents
                var chapters = [];
                for (i = 0; i < chapterLinks.length; i++) {
                    // pull out chapter ID
                    var chapterID = chapterLinks[i].dataset.chapter;
                    // grab element based on above ID
                    var chapterBeginner = document.getElementById(chapterID);
                    // push into the array
                    chapters.push(chapterBeginner);
                }

                window.onscroll = function () {
                    checkScroll()
                };

            }

            // function that compares location of course chapters relative to sidebar
            // toggles "menuActive" on menu elements if corresponding chapter is currently "active", and break if so
            function checkScroll() {

                for ( i = chapters.length; i > 0; i-- ) {
		    	var menuItem = document.querySelectorAll("[data-chapter=" + chapters[ i-1 ].id +"]");
			
                    	var rect =  chapters[ i-1 ].getBoundingClientRect();
			console.log('rect - ' + rect);
                    	var top = rect.top.toFixed();
			console.log('top -> ' + top);
                    	var chapter1 = chapters[ 0 ].getBoundingClientRect();
                        console.log('capter1 -> ' + chapter1);
                    	var ch1top = chapter1.top.toFixed();
                         console.log('capter1 top -> ' + ch1top);

                    	// grab current ToC menu element
                    	

                    	// check if chapter header is less than 200px from the top of the screen
                   	 if ( top <= 200 ) {
                        	(document.querySelector('.menuActive')) ? document.querySelector('.menuActive').classList.remove('menuActive') : '';
                        	menuItem[0].classList.add('menuActive');
                       	 	break;
                    	} else {
                        // check if we're currently looking at the first chapter...if so, default to setting that as active
                        	if ( ch1top >= 0 ) {
                           	 	menuItem = document.querySelectorAll( "[data-chapter=" + chapters[ 0 ].id + "]" );
                            		(document.querySelector('.menuActive')) ? document.querySelector('.menuActive').classList.remove('menuActive') : '';
                            		menuItem[0].classList.add('menuActive');
                           	 	break;
                        	}
                        	continue;
                    	}
		    }
	    }

        });

